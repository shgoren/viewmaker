{
    "exp_base": null,
    "exp_name": "viewmaker-disc",
    "project": "viewmaker",
    "system": "PretrainViewMakerSystemDABS",
    "cuda": true,
    "gpu_device": 0,
    "distributed_backend": "dp",
    "seed": 2,
    "data_loader_workers": 8,
    "data_root": "/disk2/ofirb/dabs/data",
    "dataset": "chexpert",
    "model_params": {
        "out_dim": 128,
        "resnet": true,
        "resnet_version": "resnet50",
        "resnet_small": false,
        "projection_head": true,
        "noise_dim": 100,
        "num_res_blocks": 3,
        "additive": 1,
        "multiplicative": 0,
        "tps": 0,
        "multiplicative_budget": 0.05,
        "additive_budget": 0.1,
        "tps_budget": 0.05,
        "budget_aware": true
    },
    "loss_params": {
        "objective": "AdversarialSimCLRLoss",
        "view_maker_loss_weight": 1,
        "t": 0.07
    },
    "optim_params": {
        "batch_size": 14,
        "learning_rate": 0.03,
        "momentum": 0.9,
        "weight_decay": 1e-4,
        "validate_freq": 1,
        "patience": 1000,
        "num_view_update": 1,
        "viewmaker_optim": "adam"
    },
    "num_epochs": 200,
    "validate": true,
    "copy_checkpoint_freq": 20,
    "continue_from_checkpoint": null,
    "val_check_interval": 300
}