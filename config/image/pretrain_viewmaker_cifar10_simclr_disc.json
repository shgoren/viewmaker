{
    "exp_base": null,
    "exp_name": "viewmaker-disc",
    "project": "viewmaker",
    "system": "PretrainViewMakerSystemDisc",
    "cuda": true,
    "gpu_device": 0,
    "distributed_backend": "ddp",
    "seed": 2,
    "data_loader_workers": 8,
    "data_params": {
        "dataset": "cifar10"
    },
    "model_params": {
        "out_dim": 100,
        "resnet": true,
        "resnet_version": "resnet18",
        "resnet_small": true,
        "projection_head": false,
        "noise_dim": 100,
        "view_bound_magnitude": 0.05,
        "num_res_blocks": 3,
        "use_budget": true,
        "budget_sched": "random_normal",
        "budget_mean": 0.05,
        "budget_aware": false,
        "viewmaker_backbone": "Viewmaker",
        "additive": 1,
        "multiplicative": 0,
        "tps": 0
    },
    "disc": {
        "dis_skip_steps": 0,
        "vm_gen_loss_weight": 0.01,
        "r1_penalty_weight": 0,
        "conv_blocks": 4,
        "wgan": false,
        "gan_warmup": 10
    },
    "loss_params": {
        "objective": "AdversarialSimCLRLoss",
        "view_maker_loss_weight": 1,
        "t": 0.07
    },
    "optim_params": {
        "batch_size": 128,
        "learning_rate": 0.03,
        "disc_learning_rate": 1e-4,
        "momentum": 0.9,
        "weight_decay": 1e-4,
        "validate_freq": 1,
        "patience": 1000,
        "num_view_update": 1,
        "viewmaker_optim": "adam"
    },
    "discriminator": true,
    "num_epochs": 200,
    "validate": true,
    "copy_checkpoint_freq": 20,
    "continue_from_checkpoint": null
}