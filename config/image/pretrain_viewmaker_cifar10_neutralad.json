{
    "exp_base": null,
    "exp_name": "cifar_neutralad_loss_v1",
    "system": "PretrainNeuTraLADViewMakerSystem",
    "cuda": true,
    "gpu_device": 0,
    "distributed_backend": "dp",
    "seed": 2,
    "data_loader_workers": 8,
    "data_params": {
        "dataset": "cifar10"
    },
    "model_params": {
        "out_dim": 128,
        "resnet": true,
        "resnet_version": "resnet18",
        "resnet_small": true,
        "projection_head": false,
        "noise_dim": 100,
        "view_bound_magnitude": 0.05,
        "num_res_blocks": 3,
        "num_views": 12,
        "project": false
    },
    "loss_params": {
        "objective": "NeuTraLADLoss",
        "view_maker_loss_weight": 1,
        "t": 1
    },
    "optim_params": {
        "batch_size": 128,
        "learning_rate": 0.03,
        "momentum": 0.9,
        "weight_decay": 1e-4,
        "validate_freq": 1,
        "patience": 200,
        "num_view_update": 1,
        "viewmaker_optim": "adam"
    },
    "num_epochs": 100,
    "validate": true,
    "copy_checkpoint_freq": 20,
    "continue_from_checkpoint": null
}